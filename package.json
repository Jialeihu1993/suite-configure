{
  "version": "0.0.1",
  "description": "ITSMA configuration UI",
  "main": "",
  "repository": {
    "type": "git",
    "url": "https://github.hpe.com/SMA-RnD/config-ui.git"
  },
  "publishConfig": {
    "registry": "https://shc-nexus-repo.hpeswlab.net:8443/repository/itsma-npm/"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "dist": "npm run clean & webpack --env=dist",
    "lint": "eslint ./src",
    "posttest": "npm run lint",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "serve": "npm run clean & node server.js --env=dev",
    "serve:dist": "npm run clean & node server.js --env=dist",
    "start": "npm run clean & node server.js --env=dev",
    "build": "npm run build:prepare-build && npm run dist && npm run build:server && npm run build:copy-files && npm run build:npm-install && npm run build:remove-npmrc",
    "build:copy-files": "babel-node ./scripts/copy-files.js",
    "build:npm-install": "cd build && npm install",
    "build:server": "babel ./app --out-dir ./build/app",
    "build:remove-npmrc": "cd build && rimraf .npmrc",
    "build:prepare-build": "npm run clean && npm run clean:build && mkdirp build",
    "clean:build": "rimraf build",
    "prebuild": "npm run clean && npm run clean:build",
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false"
  },
